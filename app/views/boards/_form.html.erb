<%= form_for @board do |f| %>
<p>
<%= f.label :date %><br>
<%= f.date_select :date %>
</p>
<p>
<%= f.label :location %><br>
<%= f.text_field :location %>
</p>
<p>
<%= f.label :image %><br>
<%= f.text_field :image %>
</p>
<p>
<%= f.label :comment %><br>
<%= f.text_field :comment %>
</p>
<p>
<%= f.submit %>
</p>
<% end %>